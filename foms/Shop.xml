<?xml version='1.0' encoding='utf-8'?>
<objectModel
		xmlns='http://standards.ieee.org/IEEE1516-2010'
		xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
		xsi:schemaLocation='http://standards.ieee.org/IEEE1516-2010 http://standards.ieee.org/downloads/1516/1516.2-2010/IEEE1516-DIF-2010.xsd'  >
	<modelIdentification>
		<name>ShopSimulation</name>
		<type>FOM</type>
		<version>1.0</version>
	</modelIdentification>
	<objects>
		<objectClass>
			<name>HLAobjectRoot</name>
			<objectClass>
				<name>CashRegister</name>
				<sharing>PublishSubscribe</sharing>
				<semantics>Kasa w sklepie</semantics>
				<attribute>
					<name>type</name>
					<dataType>HLAstring</dataType>
					<updateType>Static</updateType>
					<updateCondition>On Change</updateCondition>
					<ownership>Divest</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>Receive</order>
					<semantics>Typ kasy (zwykła lub uprzywilejowana)</semantics>
				</attribute>
				<attribute>
					<name>speed</name>
					<dataType>HLAfloat32BE</dataType>
					<updateType>Static</updateType>
					<updateCondition>On Change</updateCondition>
					<ownership>Divest</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>Receive</order>
					<semantics>Prędkość obsługi klientów w kase (czas trwania obsługi)</semantics>
				</attribute>
				<attribute>
					<name>maxLength</name>
					<dataType>HLAinteger32BE</dataType>
					<updateType>Static</updateType>
					<updateCondition>On Change</updateCondition>
					<ownership>Divest</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>Receive</order>
					<semantics>Max długość kolejki</semantics>
				</attribute>
				<attribute>
					<name>queueLen</name>
					<dataType>HLAinteger32BE</dataType>
					<updateType>Static</updateType>
					<updateCondition>On Change</updateCondition>
					<ownership>Divest</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>Receive</order>
					<semantics>Obecna długość kolejki</semantics>
				</attribute>
			</objectClass>
			<objectClass>
				<name>Customer</name>
				<sharing>PublishSubscribe</sharing>
				<semantics>Klient w sklepie</semantics>
				<attribute>
					<name>type</name>
					<dataType>HLAstring</dataType>
					<updateType>Static</updateType>
					<updateCondition>On Change</updateCondition>
					<ownership>Divest</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>Receive</order>
					<semantics>Typ klienta (zwykły lub uprzywilejowany)</semantics>
				</attribute>
				<attribute>
					<name>products</name>
					<dataType>HLAinteger32BE</dataType>
					<updateType>Static</updateType>
					<updateCondition>On Change</updateCondition>
					<ownership>Divest</ownership>
					<sharing>PublishSubscribe</sharing>
					<transportation>HLAreliable</transportation>
					<order>Receive</order>
					<semantics>Liczba produktów zakupionych przez klienta</semantics>
				</attribute>
			</objectClass>
		</objectClass>
	</objects>
	<interactions>
		<interactionClass>
			<name>HLAinteractionRoot</name>
			<interactionClass>
				<name>CustomerToQueue</name>
				<sharing>PublishSubscribe</sharing>
				<transportation>HLAreliable</transportation>
				<order>Receive</order>
				<semantics>Zdarzenie ustawienia się w kolejce do kasy</semantics>
				<parameter>
					<name>type</name>
					<dataType>HLAinteger32BE</dataType>
					<semantics>Typ kasy docelowej</semantics>
				</parameter>
			</interactionClass>
			<interactionClass>
				<name>CustomerPayment</name>
				<sharing>PublishSubscribe</sharing>
				<transportation>HLAreliable</transportation>
				<order>Receive</order>
				<semantics>Zdarzenie płatności klienta</semantics>
				<parameter>
					<name>payment</name>
					<dataType>HLAstring</dataType>
					<semantics>Sposób płatności klienta (gotówka)</semantics>
				</parameter>
			</interactionClass>
		</interactionClass>
	</interactions>
	<dimensions/>
	<synchronizations>
		<synchronizationPoint>
			<label>ReadyRoRun</label>
			<dataType>NA</dataType>
			<capability>NA</capability>
			<semantics>Waiting for each federate joining</semantics>
		</synchronizationPoint>
	</synchronizations>
	<transportations/>
	<updateRates/>
	<dataTypes>
		<basicDataRepresentations/>
		<simpleDataTypes/>
		<enumeratedDataTypes/>
		<arrayDataTypes/>
		<fixedRecordDataTypes/>
		<variantRecordDataTypes/>
	</dataTypes>
	<notes/>
</objectModel>
